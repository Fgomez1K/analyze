{
  "build": {
    "content": [
      {
        "files": [
          "sidetoc/toc.yml",
          "*.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "../docs",
    "template": [
      "default",
      "templates/html"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appFooter": "&copy; Copyright IBM Corporation 2020.",
      "_appFaviconPath": "images/favicon.png",
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableSideFilter": false,
      "_disableToc": false,
      "_gitContribute": {
        "branch": "master"
      },
      "_gitUrlPattern": "github"
    }
  }
}